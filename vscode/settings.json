{
    "go.alternateTools": {
        "gopls": "trae-gopls",
        "go.lintOnSave": "off",
    },
    "go.toolsEnvVars": {
        "GOMENLIMIT": "20GiB"
    },
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontSize": 14,
    "editor.fontLigatures": true,
    "explorer.confirmDelete": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.tree.indent": 20,
    "terminal.integrated.fontFamily": "JetBrains Mono",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.lineHeight": 1.15,
    "editor.wordWrap": "on",
    "editor.minimap.showSlider": "mouseover",
    "editor.minimap.enabled": false,
    "terminal.integrated.tabs.enabled": true,
    "terminal.external.windowsExec": "E:\\files\\tools\\Windows Terminal",
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "Windows PowerShell": {
            "path": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
        }
    },
    "security.workspace.trust.untrustedFiles": "open",
    "remote.SSH.remotePlatform": {
        "192.168.43.129": "linux"
    },
    "editor.stickyScroll.enabled": false,
    "vim.incsearch": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.leader": "<space>",
    "vim.foldfix": true,
    "vim.smartRelativeLine": true,
    "vim.useSystemClipboard": true,
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-j>"
            ],
            "commands": [
                "selectNextSuggestion"
            ]
        },
        {
            "before": [
                "<C-k>"
            ],
            "commands": [
                "selectPrevSuggestion"
            ],
        },
        {
            "before": [
                "<C-i>"
            ],
            "commands": [
                "editor.action.triggerParameterHints"
            ],
        },
        {
            "before": [
                "<C-p>"
            ],
            "commands": [
                "closeParameterHints"
            ],
        },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "y"
            ],
            "after": [
                "y",
                "`",
                ">"
            ]
        },
        {
            "before": [
                "<C-h>"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "<C-j>"
            ],
            "after": [
                "1",
                "0",
                "j"
            ]
        },
        {
            "before": [
                "<C-k>"
            ],
            "after": [
                "1",
                "0",
                "k"
            ]
        },
        {
            "before": [
                ":"
            ],
            "after": [
                ";"
            ]
        },
        {
            "before": [
                ";"
            ],
            "after": [
                ":"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-o>"
            ],
            "commands": [
                "workbench.action.navigateBack"
            ]
        },
        {
            "before": [
                "<C-i>"
            ],
            "commands": [
                "workbench.action.navigateForward"
            ]
        },
        {
            "before": [
                "leader",
                "f",
                "D"
            ],
            "commands": [
                "editor.action.goToTypeDefinition"
            ]
        },
        {
            "before": [
                "leader",
                "p",
                "'"
            ],
            "after": [
                "v",
                "i",
                "'",
                "p"
            ]
        },
        {
            "before": [
                "leader",
                "p",
                "\""
            ],
            "after": [
                "v",
                "i",
                "\"",
                "p"
            ]
        },
        {
            "before": [
                "leader",
                "f",
                "d"
            ],
            "after": [
                "<C-]>"
            ]
        },
        {
            "before": [
                "leader",
                "f",
                "r"
            ],
            "commands": [
                "editor.action.goToReferences"
            ]
        },
        {
            "before": [
                "leader",
                "f",
                "i"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ]
        },
        {
            "before": [
                "leader",
                "n"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "leader",
                "g",
                "n"
            ],
            "commands": [
                "workbench.action.editor.nextChange"
            ]
        },
        {
            "before": [
                "leader",
                "g",
                "N"
            ],
            "commands": [
                "workbench.action.editor.previousChange"
            ]
        },
        {
            "before": [
                "leader",
                "g",
                "p"
            ],
            "commands": [
                "editor.action.dirtydiff.next"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        },
        {
            "before": [
                "<Esc>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                ";"
            ],
            "after": [
                ":"
            ]
        },
        {
            "before": [
                ":"
            ],
            "after": [
                ";"
            ]
        },
        {
            "before": [
                "<Tab>"
            ],
            "after": [
                "/"
            ]
        },
        {
            "before": [
                "<S-Tab>"
            ],
            "after": [
                "?"
            ]
        },
        {
            "before": [
                "<C-L>"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "<C-h>"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "<C-k>"
            ],
            "after": [
                "1",
                "0",
                "k"
            ]
        },
        {
            "before": [
                "<C-j>"
            ],
            "after": [
                "1",
                "0",
                "j"
            ]
        },
        {
            "before": [
                "leader",
                "l"
            ],
            "commands": [
                "workbench.files.action.showActiveFileInExplorer"
            ]
        },
        {
            "before": [
                "leader",
                "f",
                "s"
            ],
            "commands": [
                "workbench.action.quickTextSearch"
            ]
        },
        {
            "before": [
                "leader",
                "f",
                "f"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "leader",
                "r",
                "n"
            ],
            "commands": [
                "editor.action.rename"
            ],
        },
        {
            "before": [
                "leader",
                "c",
                "a"
            ],
            "commands": [
                "editor.action.quickFix"
            ],
        },
        {
            "before": [
                "leader",
                "F"
            ],
            "commands": [
                "editor.action.formatDocument",
                "editor.action.organizeImports"
            ]
        }
    ],
    "editor.formatOnSave": true,
    "editor.suggestSelection": "recentlyUsed",
    "editor.suggest.shareSuggestSelections": true,
    "gopls": {},
    "go.disableConcurrentTests": true,
    "go.testFlags": [
        "-gcflags=all=-l"
    ],
    "editor.unicodeHighlight.nonBasicASCII": false,
    "git.openRepositoryInParentFolders": "never",
    "workbench.settings.applyToAllProfiles": [],
    "AI.toolcall.v2.command.allowList": "[\"npm\"]",
    "editor.inlayHints.enabled": "off",
    "[go]": {
        "editor.insertSpaces": false,
        "editor.formatOnSave": true,
    },
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.cursorBlinking": "solid",
    "editor.renderWhitespace": "none",
    "editor.largeFileOptimizations": true,
    "editor.smoothScrolling": false,
    "editor.scrollBeyondLastLine": false,
    "editor.tabCompletion": "on",
    "workbench.colorTheme": "Default Light Modern",
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1
    },
    "editor.tokenColorCustomizations": {
        "[Default Light Modern]": {
            "comments": "#6E7681",
            "strings": "#4CAF50"
        }
    },
    "explorer.autoReveal": false
}